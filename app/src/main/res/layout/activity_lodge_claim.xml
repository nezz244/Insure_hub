<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lodge_claim">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="100dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/dollar2"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="679dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="51dp"
        android:layout_height="49dp"
        app:srcCompat="@drawable/user"
        tools:layout_editor_absoluteX="320dp"
        tools:layout_editor_absoluteY="679dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="75dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/home_icon"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="680dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="74dp"
        android:layout_height="53dp"
        app:srcCompat="@drawable/options"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="679dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="289dp"
        android:layout_height="104dp"
        app:srcCompat="@mipmap/insure_hub_foreground"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="2dp" />

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="205dp"
        android:fillViewport="true"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="279dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="10dp"
                app:srcCompat="@drawable/vehicle_insurance" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="10dp"
                app:srcCompat="@drawable/home_icon" />
        </LinearLayout>
    </HorizontalScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>